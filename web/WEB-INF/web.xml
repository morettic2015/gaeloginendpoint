<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>gaeendpoint</servlet-name>
        <servlet-class>br.com.morettic.gaelogin.GaeEndPoint</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InfoSegController</servlet-name>
        <servlet-class>br.com.morettic.gaelogin.InfoSegController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InfoSegUploadController</servlet-name>
        <servlet-class>br.com.morettic.gaelogin.InfoSegUploadController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>gaeendpoint</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InfoSegController</servlet-name>
        <url-pattern>/infosegcontroller.exec</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InfoSegUploadController</servlet-name>
        <url-pattern>/upload.exec</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
