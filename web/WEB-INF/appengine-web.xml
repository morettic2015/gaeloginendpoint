<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>gaeloginendpoint</application>
    <version>capsule-corp-30</version>
    <threadsafe>true</threadsafe>
    <sessions-enabled>true</sessions-enabled>
    <!--<instance-class>B8</instance-class>
    <basic-scaling>
        <max-instances>1</max-instances>
        <idle-timeout>10m</idle-timeout>
    </basic-scaling> -->
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
        <property name="appengine.api.urlfetch.defaultDeadline" value="60"/>
        <property name="appengine.datastore.disableChunkSizeWarning" value="true"/>
    </system-properties>
    <static-files>
        <include path="/images/**.png" />
        <include path="/**.html" />
    </static-files>
</appengine-web-app>
